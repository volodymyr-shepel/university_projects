      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{0BD56455-8A16-4B7C-A29C-3C5E09608C5C}"
          SQLTask:SqlStatementSource="DECLARE @CrimesWeatherExists BIT;&#xA;DECLARE @HistoryCrimesWeatherExists BIT;&#xA;DECLARE @SourceTableExists BIT;&#xA;&#xA;-- Check if the Crimes_Weather schema exists&#xA;SELECT @CrimesWeatherExists = CASE &#xA;                                 WHEN EXISTS (SELECT * &#xA;                                              FROM INFORMATION_SCHEMA.SCHEMATA &#xA;                                              WHERE SCHEMA_NAME = 'Crimes_Weather') &#xA;                                 THEN 1 &#xA;                                 ELSE 0 &#xA;                              END;&#xA;&#xA;-- Check if the History_Crimes_Weather schema exists&#xA;SELECT @HistoryCrimesWeatherExists = CASE &#xA;                                        WHEN EXISTS (SELECT * &#xA;                                                     FROM INFORMATION_SCHEMA.SCHEMATA &#xA;                                                     WHERE SCHEMA_NAME = 'History_Crimes_Weather') &#xA;                                        THEN 1 &#xA;                                        ELSE 0 &#xA;                                     END;&#xA;&#xA;-- Check if the source table exists&#xA;-- Replace 'YourSourceTableName' and 'YourSourceSchema' with your actual table and schema names&#xA;SELECT @SourceTableExists = CASE &#xA;                               WHEN EXISTS (SELECT * &#xA;                                            FROM INFORMATION_SCHEMA.TABLES &#xA;                                            WHERE TABLE_NAME = 'SourceTable' &#xA;                                              AND TABLE_SCHEMA = 'dbo') &#xA;                               THEN 1 &#xA;                               ELSE 0 &#xA;                            END;&#xA;&#xA;-- Determine if the DB setup task should run&#xA;-- If any of the schemas or the source table do not exist, set ExecuteDBSetup to 1 (True)&#xA;SELECT CASE &#xA;         WHEN @CrimesWeatherExists = 0 OR @HistoryCrimesWeatherExists = 0 OR @SourceTableExists = 0 &#xA;         THEN 1 &#xA;         ELSE 0 &#xA;       END AS ExecuteDBSetup;"
          SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ResultBinding
            SQLTask:ResultName="ExecuteDBSetup"
            SQLTask:DtsVariableName="User::isExecuteDBSetup" />
        </SQLTask:SqlTaskData>
